int main (){
    float phy, math,chem,eng,hindi;
    float total_marks,average_marks,percentage_marks;

    printf("Enter marks of 5 subject");
    scanf("%f%f%f%f%f",&phy,&math,&chem,&eng,&hindi);

    total_marks = phy +math + chem + eng + hindi;
    average_marks = total_marks/5;
    percentage_marks = (total_marks/500)*100;

    printf("total marks %.2f\n",total_marks);
    printf("average marks%.2f\n",average_marks);
    printf("percentage marks%.2f",percentage_marks);

    return 0;

}
